              having sum(p.dec_acct_amount) &gt; 0) p1,&#13;&#10;               (select p.dec_acct_detail_seq, p.dec_positionseq&#13;&#10;                  from suspend_records p) p2&#13;&#10;         where p2.dec_positionseq = p1.dec_positionseq);&#13;&#10;&#13;&#10;         &#13;&#10;COMMIT;&#13;&#10;&#13;&#10;&#13;&#10;update out_acct_detail acct_upt&#13;&#10;   set acct_upt.dte_release = (SELECT MAX(TO_DATE(txt_key_value,'YYYY-MM-DD')) FROM in_etl_control WHERE txt_key_string = 'OPER_CYCLE_DATE' AND txt_file_name ='GLOBAL') &#13;&#10;where acct_upt.dec_acct_detail_seq in&#13;&#10;(select dtl.dec_acct_detail_seq&#13;&#10;from&#13;&#10;(&#13;&#10;select p.dte_cycle_date, &#13;&#10;       p.txt_processingunit, &#13;&#10;       p.dec_positionseq,&#13;&#10;       p.txt_position_name,&#13;&#10;       p.txt_acct_currency&#13;&#10;  from (select acct.dte_cycle_date, acct.txt_processingunit, acct.dec_acct_detail_seq, acct.dec_positionseq, acct.txt_position_name, acct.txt_acct_currency, acct.dec_acct_amount&#13;&#10;          from out_acct_detail acct&#13;&#10;         where txt_status = 'ACTIVE'&#13;&#10;           and txt_acct_type = 'P'&#13;&#10;           and acct.txt_earning_group = acct.txt_acct_currency&#13;&#10;           and exists&#13;&#10;         (select 1&#13;&#10;            from (select inctrl.*&#13;&#10;                    from in_etl_control inctrl&#13;&#10;                   where inctrl.txt_key_string in ('OPER_CYCLE_DATE', '$$STR_ACCT_CYCLE_TYPE')&#13;&#10;                     and inctrl.txt_file_name in ('GLOBAL', '$$STR_PROCESSING_UNIT')) inctrl&#13;&#10;           where acct.dte_cycle_date = (case when inctrl.txt_key_string = 'OPER_CYCLE_DATE' then to_date(inctrl.txt_key_value, 'yyyy-mm-dd') end)&#13;&#10;              or (acct.txt_cycle_type = (case when inctrl.txt_key_string = '$$STR_ACCT_CYCLE_TYPE' then inctrl.txt_key_value end) and acct.dte_cycle_date = (case when inctrl.txt_key_string = 'OPER_CYCLE_DATE' then to_date(inctrl.txt_key_value, 'yyyy-mm-dd') end))&#13;&#10;              or (acct.txt_cycle_type = (case when inctrl.txt_key_string = '$$STR_ACCT_CYCLE_TYPE' then inctrl.txt_file_name end) and acct.dte_cycle_date = (case when inctrl.txt_key_string = 'OPER_CYCLE_DATE' then to_date(inctrl.txt_key_value, 'yyyy-mm-dd') end)))) p&#13;&#10; group by p.dte_cycle_date,&#13;&#10;          p.txt_processingunit,&#13;&#10;          p.dec_positionseq,&#13;&#10;          p.txt_position_name,&#13;&#10;          p.txt_acct_currency&#13;&#10;having sum(p.dec_acct_amount) &gt; 0) suspend_records,&#13;&#10;out_acct_detail dtl&#13;&#10;where dtl.txt_status = 'ACTIVE'&#13;&#10;   and dtl.txt_acct_type = 'P'&#13;&#10;   and dtl.dte_suspend is not null&#13;&#10;   and dtl.txt_processingunit = suspend_records.txt_processingunit&#13;&#10;   and dtl.dte_cycle_date &lt;= suspend_records.dte_cycle_date&#13;&#10;   and dtl.dec_positionseq = suspend_records.dec_positionseq&#13;&#10;   and dtl.dte_release is null);&#13;&#10;&#13;&#10;COMMIT;" />
